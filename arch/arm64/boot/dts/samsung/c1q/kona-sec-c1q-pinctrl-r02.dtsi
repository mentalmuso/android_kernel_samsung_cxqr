#include "../drivers/audio_canvas_pinctrl_common.dtsi"
#include "../drivers/kona-sec-canvas-tsp-pinctrl.dtsi"
#include "../drivers/kona-sec-c2q-mfc-pinctrl.dtsi"
#include "../drivers/kona-sec-c1q-camera-sensor-pinctrl-r02.dtsi"
#include "../drivers/bluetooth_canvas_pinctrl_common.dtsi"
#include "../drivers/kona-sec-hubble-interposer.dtsi"
#include "../drivers/connector_detection_c1_pinctrl_common.dtsi"

&tlmm {
	bcmdhd_default: bcmdhd_default {
		mux {
			pins = "gpio128";
			function = "gpio";
		};
		config {
			pins = "gpio128";
			drive-strength = <2>;   /* 2 mA */
			bias-pull-down;         /* PULL DOWN */
		};
	};

	bcmdhd_wlan_en: bcmdhd_wlan_en {
		mux {
			pins = "gpio90";
			function = "gpio";
		};
		config {
			pins = "gpio90";
			drive-strength = <2>;   /* 2 mA */
			bias-pull-down;         /* PULL DOWN */
		};
	};

	grip_int_active: grip_int_active {
		mux {
			pins = "gpio111";
			function = "gpio";
		};
		config {
			pins = "gpio111";
			drive-strength = <2>;
			bias-disable;
		};
	};

	grip_i2c_active: grip_i2c_active {
		mux {
			pins = "gpio112", "gpio113";
			function = "gpio";
		};
		config {
			pins = "gpio112", "gpio113";
			drive-strength = <2>;
			bias-disable;
		};
	};

	bt_wake_default: bt_wake_default {
		mux {
			pins = "gpio124";
			function = "gpio";
		};
		config {
			pins = "gpio124";
			drive-strength = <2>;
			bias-pull-down;
		};
	};

	bt_hostwake_default: bt_hostwake_default {
		mux {
			pins = "gpio27";
			function = "gpio";
		};
		config {
			pins = "gpio27";
			drive-strength = <2>;
			bias-pull-down;
		};
	};

};

&pm8150l_gpios {
    ap2mdm_errfatal2_default: ap2mdm_errfatal2_default {
        pins = "gpio7";
        function = "normal";
        power-source = <1>;
        bias-disable;
        output-low;
        input-disable;
    };
};
