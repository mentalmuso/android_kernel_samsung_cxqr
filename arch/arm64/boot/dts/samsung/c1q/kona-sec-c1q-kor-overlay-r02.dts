/dts-v1/;
/plugin/;

#include "../common/kona-sec-common.dtsi"
#include "kona-sec-c1q-r02.dtsi"
#include "../drivers/fingerprint-sensor_c1q_r02.dtsi"

/ {
	model = "Samsung C1Q PROJECT - C1_NA_REV0.0_BRING-up (board-id,2)";
	compatible = "qcom,kona-mtp", "qcom,kona", "qcom,mtp";
	qcom,board-id = <8 2>;
	dtbo-version = <0>;
};

&soc {
	i2c@17 { /* SW I2C */
		status = "ok";
		sx9360-i2c@28 {
			sx9360,proxthresh_mcc = <0x22>; /* threshold : 578 */
		};
	};
};
