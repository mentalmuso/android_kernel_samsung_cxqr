/dts-v1/;
/plugin/;

#include "../common/kona-sec-common.dtsi"
#include "../common/kona-sec-laa-chn-jpn.dtsi"

#include "kona-sec-x1q-r09.dtsi"

/ {
	model = "Samsung X1Q CHN PROJECT - DVR Rev0.2 (board-id,09)";
	compatible = "qcom,kona-mtp", "qcom,kona", "qcom,mtp";
	qcom,board-id = <8 9>;
	dtbo-version = <0>;
};

&soc {
	i2c@17 { /* SW I2C */
		status = "ok";
		sx9360-i2c@28 {
			sx9360,proxthresh_reg = <0x18>; /* threshhold : 288 */
		};
	};
};
