&tlmm {
	attn_irq: attn_irq {
		mux {
			pins = "gpio15";
			function = "gpio";
		};
		config {
			pins = "gpio15";
			input-enable;
			bias-disable;
		};
	};

	sub_tsp_int: sub_tsp_int {
		mux {
			pins = "gpio14";
			function = "gpio";
		};
		config {
			pins = "gpio14";
			input-enable;
			bias-disable;
		};
	};

	/* SE 5 pin mappings */
	qupv3_se5_tsp_i2c_pins: qupv3_se5_tsp_i2c_pins {
		qupv3_se5_tsp_i2c_active: qupv3_se5_tsp_i2c_active {
			mux {
				pins = "gpio12", "gpio13";
				function = "qup5";
			};

			config {
				pins = "gpio12", "gpio13";
				drive-strength = <2>;
				bias-disable;
			};
		};

		qupv3_se5_tsp_i2c_sleep: qupv3_se5_tsp_i2c_sleep {
			mux {
				pins = "gpio12", "gpio13";
				function = "gpio";
			};

			config {
				pins = "gpio12", "gpio13";
				drive-strength = <2>;
				bias-disable;
			};
		};
	};

	/* SE 13 pin mappings */
	qupv3_se13_tsp_i2c_pins: qupv3_se13_tsp_i2c_pins {
		qupv3_se13_tsp_i2c_active: qupv3_se13_tsp_i2c_active {
			mux {
				pins = "gpio36", "gpio37";
				function = "qup13";
			};

			config {
				pins = "gpio36", "gpio37";
				drive-strength = <2>;
				bias-disable;
			};
		};

		qupv3_se13_tsp_i2c_sleep: qupv3_se13_tsp_i2c_sleep {
			mux {
				pins = "gpio36", "gpio37";
				function = "gpio";
			};

			config {
				pins = "gpio36", "gpio37";
				drive-strength = <2>;
				bias-disable;
			};
		};
	};
};
